{
  "name": "vue-promise-builder",
  "version": "1.0.5",
  "description": "Component that builds itself based on the latest snapshot of interaction with a Promise.",
  "repository": {
    "type": "git",
    "url": "https://github.com/c5n8/vue-promise-builder"
  },
  "homepage": "https://github.com/c5n8/vue-promise-builder#readme",
  "license": "MIT",
  "keywords": [
    "vue",
    "promise",
    "builder",
    "component"
  ],
  "main": "dist/vue-promise-builder.ssr.js",
  "browser": "dist/vue-promise-builder.esm.js",
  "module": "dist/vue-promise-builder.esm.js",
  "unpkg": "dist/vue-promise-builder.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "lodash-es": "^4.17.15",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {}
}
